{
	"compilerOptions": {
		"lib": ["ESNext", "DOM"],
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "Node16",
		"allowJs": true,
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noPropertyAccessFromIndexSignature": false,
		"types": ["tampermonkey"]
	}
}
